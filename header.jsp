<%@page  pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <style>
      *{
        margin: 0;
        padding: 0;
        box-sizing: border-box;
      }
      .header {
        width: 100vw;
         background-color: #4297A0;
      }
      .image {
        padding-inline: 40px;
        width: 100%;
      }
      .flex {
        font-size: 1.2rem;
        display: flex;
        justify-content: end;
        padding: 0 20px 20px 0;
      }
    </style>
  </head>
  <body>
    <div class="header">
      <img src="./college-logo.png" class="image" alt="college-logo" />
      <span class="flex"><p>Made by: Arpit Sharma</p></span>
    </div>
  </body>
</html>
